#include<stdio.h>
struct fraction
{
	int numarator;
	int denominator;
};
void input_fraction(struct fraction *f1)
{
	printf("enter the numarator");
	scanf("%d",f1->numarator);
	printf("enter the denominator");
	scanf("%d",f1->denominator);
}
void input_fraction2(struct fraction *f2)
{
	printf("enter the numarator");
	scanf("%d",f2->numarator);
	printf("enter the denominator");
	scanf("%d",f2->denominator);
}
