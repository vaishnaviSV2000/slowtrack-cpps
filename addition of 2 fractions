#include<stdio.h>
struct fraction
{
	int num;
	int den;
};
struct input_fraction1(struct fraction *f1)
{
	printf("enter the value of numarator");
	scanf("%d",&num->f1);
	printf("enter the value of denominator");
	scanf("%d",&den->f1);
}
struct input_fraction2(struct fraction *f2)
{
	printf("enter the value of numarator");
	scanf("%d",&num->f2);
	printf("enter the value of denominator");
	scanf("%d",&den->f2);
}/*
